"\ttime_t  fail_time;\n"
"\tlong    fail_locktime;\n"
"};"
msgstr ""
"\n"
"struct\tfaillog {\n"
"\tshort   fail_cnt;\n"
"\tshort   fail_max;\n"
"\tchar    fail_line[12];\n"
"\ttime_t  fail_time;\n"
"\tlong    fail_locktime;\n"
"};"


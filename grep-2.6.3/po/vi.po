msgstr "dữ liệu nhập quá lớn để đếm được"

#: src/main.c:842
#, c-format
msgid "writing output"
msgstr "đang ghi kết xuất"

#: src/main.c:1165
#, c-format
msgid "Binary file %s matches\n"
msgstr "Tập tin nhị phân %s tương ứng với\n"

#: src/main.c:1179
msgid "(standard input)"
msgstr "(đầu vào tiêu chuẩn)"

#: src/main.c:1310
#, c-format
msgid "warning: %s: %s\n"
